    "axios": "^1.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "recharts": "^3.0.2",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "babel-plugin-import": "^1.13.8",
    "compression-webpack-plugin": "^10.0.0",
    "customize-cra": "^1.0.0",
    "react-app-rewired": "^2.2.1",
    "terser-webpack-plugin": "^5.3.14",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
